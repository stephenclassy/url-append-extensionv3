{
   "background": {
      "persistent": false,
      "scripts": [ "defaults.js", "bg.js" ]
   },
   "browser_action": {
   },
   "description": "Quickly copy a Microsoft Learn URL, with your desired tracking parameter appended.",
   "icons": {
      "16": "icon/learnAppenderIcon16.png",
      "32": "icon/learnAppenderIcon32.png",
      "48": "icon/learnAppenderIcon48.png",
      "128": "icon/learnAppenderIcon128.png"
   },
   "manifest_version": 2,
   "name": "Microsoft Learn URL Appender",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "activeTab", "clipboardWrite", "contextMenus", "storage", "*://*.lightning.force.com/*"],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1"
}
